{
  "name": "pixi-extra-filters",
  "version": "1.1.2",
  "description": "Extra pixi filters not included by default with the main library",
  "author": "Chad Engler <chad@pantherdev.com>",
  "contributors": [
    "Mat Groves",
    "Richard Davey <rdavey@gmail.com>"
  ],
  "main": "./src/index.js",
  "homepage": "https://github.com/pixijs/pixi-extra-filters",
  "bugs": {
    "url": "https://github.com/pixijs/pixi-extra-filters/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pixijs/pixi-extra-filters.git"
  },
  "keywords": [
    "pixi",
    "filters",
    "2d",
    "render",
    "glsl",
    "shader",
    "glow",
    "outline"
  ],
  "scripts": {
    "clean": "rimraf bin && mkdirp bin",
    "prestart": "npm run clean",
    "start": "parallelshell \"npm run watch:lint\" \"npm run watch\"",
    "watch": "pixify -o pixi-extra-filters -w",
    "watch:lint": "watch \"jshint --reporter=scripts/reporter.js scripts src test || exit 0\" src",
    "lint": "jshint --reporter=scripts/reporter.js scripts src",
    "prebuild": "npm run lint && npm run clean",
    "build": "pixify -o pixi-extra-filters",
    "docs": "jsdoc -c scripts/jsdoc.conf.json -R README.md"
  },
  "files": [
    "bin/",
    "src/",
    "CONTRIBUTING.md",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "dependencies": {
    "glslify": "^5.0.2",
    "browserify-versionify": "^1.0.6"
  },
  "devDependencies": {
    "del": "^2.2.0",
    "jaguarjs-jsdoc": "^1.0.1",
    "jsdoc": "^3.4.0",
    "jshint": "^2.9.2",
    "jshint-stylish": "^2.2.0",
    "jshint-stylish-summary": "^0.1.2",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "parallelshell": "^2.0.0",
    "pixify": "^1.4.0",
    "rimraf": "^2.5.3",
    "watch": "^0.19.1"
  },
  "browserify": {
    "transform": [
      "glslify",
      "browserify-versionify"
    ]
  }
}
